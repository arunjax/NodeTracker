<div class="page-header">
  <h1>Edit user</h1>
</div>

<% form_for(member, {action: path_to.user(member), method: 'PUT', id: "user_form"}, function (form) { %>
  <%- partial('users/form.ejs', {locals: {form: form, user: member}}) %>
  <div class="actions">
    <%- form.submit('Update user', {class: 'btn primary'}) %> or
    <%- link_to('Cancel', path_to.user(member), {class: 'btn'}) %>
  </div>
<% });%>
